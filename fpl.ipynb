    "        \n",
    "        # Calculate form (last 5 GWs)\n",
    "        last_5_gws = player_gw_data.tail(5)\n",
    "        form_stats = {\n",
    "            \"avg_points\": round(float(last_5_gws['total_points'].mean()), 1),\n",
    "            \"avg_minutes\": round(float(last_5_gws['minutes'].mean()), 0),\n",
