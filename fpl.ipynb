    "                row['season_key_passes_z'] * 0.15 +\n",
    "                row['points_per_million_z'] * 0.1\n",
    "            )\n",
    "        elif pos == 'DEF':\n",
    "            return (\n",
    "                row['xCS_per_game_z'] * 0.3 +\n",
    "                row['xA_per_game_z'] * 0.15 +\n",
    "                row['form_z'] * 0.25 +\n",
    "                row['defensive_contribution_sum_z'] * 0.2 +\n",
    "                row['points_per_million_z'] * 0.1\n",
