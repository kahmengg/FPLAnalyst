    "                'swing_category': swing_category,\n",
    "                'swing_emoji': swing_emoji,\n",
    "                'form_context': form_context\n",
    "            })\n",
    "        \n",
    "        summary_data = sorted(team_summary, key=lambda x: x['overall_difficulty'], reverse=True)\n",
